<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="417dp"
        android:layout_height="90dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginTop="30sp"
            android:layout_weight="1"
            android:text="Notes App"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="156dp"
            android:layout_height="109dp"
            android:layout_marginRight="20sp"
            app:srcCompat="@drawable/noteslogo" />

    </LinearLayout>


    <ScrollView
        android:layout_width="410dp"
        android:layout_height="613dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    app:cardCornerRadius="12dp"
                    app:cardElevation="12dp"
                    app:cardUseCompatPadding="true"
                    app:contentPadding="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imageView7"
                            android:layout_width="150dp"
                            android:layout_height="100dp"
                            app:srcCompat="@drawable/htmlpg" />

                        <Button
                            android:id="@+id/htmlvidlect"
                            android:layout_width="match_parent"
                            android:layout_height="55dp"
                            android:text="VIDEO LEC" />

                        <Button
                            android:id="@+id/htmlwnlec"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Written" />
                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    app:cardCornerRadius="12dp"
                    app:cardElevation="12dp"
                    app:cardUseCompatPadding="true"
                    app:contentPadding="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imageView7"
                            android:layout_width="150dp"
                            android:layout_height="100dp"
                            app:srcCompat="@drawable/css" />

                        <Button
                            android:id="@+id/htmlvidlect"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="VIDEO LEC" />

                        <Button
                            android:id="@+id/htmlwnlec"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Written" />
                    </LinearLayout>

                </androidx.cardview.widget.CardView>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    app:cardCornerRadius="12dp"
                    app:cardElevation="12dp"
                    app:cardUseCompatPadding="true"
                    app:contentPadding="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imageView7"
                            android:layout_width="150dp"
                            android:layout_height="100dp"
                            app:srcCompat="@drawable/javasc" />

                        <Button
                            android:id="@+id/htmlvidlect"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="VIDEO LEC" />

                        <Button
                            android:id="@+id/htmlwnlec"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Written" />
                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    app:cardCornerRadius="12dp"
                    app:cardElevation="12dp"
                    app:cardUseCompatPadding="true"
                    app:contentPadding="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imageView7"
                            android:layout_width="150dp"
                            android:layout_height="100dp"
                            app:srcCompat="@drawable/phpp" />

                        <Button
                            android:id="@+id/htmlvidlect"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="VIDEO LEC" />

                        <Button
                            android:id="@+id/htmlwnlec"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Written" />
                    </LinearLayout>

                </androidx.cardview.widget.CardView>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    app:cardCornerRadius="12dp"
                    app:cardElevation="12dp"
                    app:cardUseCompatPadding="true"
                    app:contentPadding="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imageView7"
                            android:layout_width="150dp"
                            android:layout_height="100dp"
                            app:srcCompat="@drawable/texticon" />

                        <Button
                            android:id="@+id/htmlvidlect"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="VIDEO LEC" />

                        <Button
                            android:id="@+id/htmlwnlec"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Written" />
                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    app:cardCornerRadius="12dp"
                    app:cardElevation="12dp"
                    app:cardUseCompatPadding="true"
                    app:contentPadding="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imageView7"
                            android:layout_width="150dp"
                            android:layout_height="100dp"
                            app:srcCompat="@drawable/videoicon" />

                        <Button
                            android:id="@+id/htmlvidlect"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="VIDEO LEC" />

                        <Button
                            android:id="@+id/htmlwnlec"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Written" />
                    </LinearLayout>

                </androidx.cardview.widget.CardView>


            </LinearLayout>


        </LinearLayout>

    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>



